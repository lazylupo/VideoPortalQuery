<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:dicom="urn:medico:dicom:schema:2011" elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="urn:medico:dicom:schema:2011">
	<element name="Dicom" type="dicom:DicomRootType"/>
	<complexType name="DicomRootType">
		<sequence>
			<element name="Conditions" type="dicom:DicomCondition" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<complexType name="DicomCondition">
		<sequence>
			<element  name="key" type="string" />
			<element  name="operator" type="dicom:ConditionOperator" />
			<element  name="value" type="string" />
		</sequence>
	</complexType>
	<simpleType name="ConditionOperator">
	  <restriction base="string">
	    <enumeration value="EQUAL"/>
	    <enumeration value="GREATER"/>
	    <enumeration value="GREATEREQUAL"/>
	    <enumeration value="LESS"/>
	    <enumeration value="LESSEQUAL"/>
	  </restriction>
	</simpleType>
</schema>