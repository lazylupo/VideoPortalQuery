	<!-- Simple data types -->
	<!-- Only use Strings anymore -->
	<simpleType name="onlyString">
		<restriction base="string"/>
	</simpleType>
	<!--FOLLOWING PART LOOKS LIKE REMAIN UNUSED -->
	<!-- Application Entity -->
	<simpleType name="AE">
		<restriction base="string">
			<maxLength value="16"/>
		</restriction>
	</simpleType>
	<!-- Age String -->
	<simpleType name="AS">
		<restriction base="string">
			<length value="4"/>
			<pattern value="[0-9][0-9][0-9](D|W|M|Y)"/>
		</restriction>
	</simpleType>
	<!-- Coded String -->
	<simpleType name="CS">
		<restriction base="token">
			<maxLength value="16"/>
		</restriction>
	</simpleType>
	<!-- Date -->
	<simpleType name="DA">
		<restriction base="date"/>
	</simpleType>
	<!-- DecimalString -->
	<simpleType name="DS">
		<restriction base="decimal"/>
	</simpleType>
	<!-- DateTime -->
	<simpleType name="DT">
		<restriction base="dateTime"/>
	</simpleType>
	<!-- FloatingPoint Single -->
	<simpleType name="FL">
		<restriction base="decimal"/>
	</simpleType>
	<!-- FloatingPoint Double -->
	<simpleType name="FD">
		<restriction base="decimal"/>
	</simpleType>
	<!-- Integer String -->
	<simpleType name="IS">
		<restriction base="integer"/>
	</simpleType>
	<!-- Long string -->
	<simpleType name="LO">
		<restriction base="string">
			<maxLength value="64"/>
		</restriction>
	</simpleType>
	<!-- Long text -->
	<simpleType name="LT">
		<restriction base="string">
			<maxLength value="10240"/>
		</restriction>
	</simpleType>
	<!-- Other Byte String -->
	<simpleType name="OB">
		<restriction base="string"/>
	</simpleType>
	<!-- Other Float String -->
	<simpleType name="OF">
		<restriction base="string"/>
	</simpleType>
	<!-- Other Word String -->
	<simpleType name="OW">
		<restriction base="string"/>
	</simpleType>
	<!-- Person Name -->
	<simpleType name="PN">
		<restriction base="token">
			<maxLength value="64"/>
		</restriction>
	</simpleType>
	<!-- dicom:SHort String -->
	<simpleType name="SH">
		<restriction base="string">
			<maxLength value="16"/>
		</restriction>
	</simpleType>
	<!-- Signed Long -->
	<simpleType name="SL">
		<restriction base="long"/>
	</simpleType>
	<!-- Signed dicom:SHort -->
	<simpleType name="SS">
		<restriction base="short"/>
	</simpleType>
	<!-- dicom:SHort Text -->
	<simpleType name="ST">
		<restriction base="string">
			<maxLength value="1024"/>
		</restriction>
	</simpleType>
	<!-- Time -->
	<simpleType name="TM">
		<restriction base="time"/>
	</simpleType>
	<!-- dicom:UId -->
	<simpleType name="UI">
		<restriction base="token">
			<maxLength value="64"/>
			<pattern value="([0-9]+\.)*[0-9]+"/>
		</restriction>
	</simpleType>
	<!-- Unsigned dicom:SHort -->
	<simpleType name="US">
		<restriction base="unsignedShort"/>
	</simpleType>
	<!-- Unsigned Long -->
	<simpleType name="UL">
		<restriction base="unsignedLong"/>
	</simpleType>
	<!-- Unknown -->
	<simpleType name="UN">
		<restriction base="string"/>
	</simpleType>
	<!-- Unlimited Text -->
	<simpleType name="UT">
		<restriction base="string"/>
	</simpleType>
	<!-- SQ -->
	<simpleType name="SQ">
		<restriction base="string"/>
	</simpleType>
</schema>
